<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ecDAO">
	<insert id="insertEcInfo" parameterType="ecVO" >
		INSERT INTO  employee_cont_tb(
			employer_nm
			,pay
			,company_nm
			,content
			,work_start_dt
			,work_end_dt
			,cont_start_dt
			,cont_end_dt
			,reg_dt
			,mod_dt
		)
		VALUES(
			#{employerNm}
			,#{pay}
			,#{companyNm}
			,#{content}
			,#{workStartDt}
			,#{workEndDt}
			,#{contStartDt}
			,#{contEndDt}
			,now()
			,now()
		
		)
			
	
	
	</insert>

</mapper>